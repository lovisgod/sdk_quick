    <androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/iswColorPrimary">

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/isw_guideline"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.15" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/isw_guideline2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_begin="4dp"/>

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/isw_guideline3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_end="4dp"/>


    <ImageView
        android:id="@+id/isw_back"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="45dp"
        app:layout_constraintBottom_toTopOf="@id/isw_guideline"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat = "@drawable/ic_baseline_arrow_back_ios_24"
        android:layout_marginStart="20dp"
        android:layout_marginLeft="20dp" />

        <TextView
            android:id="@+id/isw_merchant_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="10dp"
            android:textColor="@android:color/white"
            android:textSize="20sp"
            android:fontFamily="@font/roboto_bold"
            app:layout_constraintBottom_toTopOf="@id/isw_guideline"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="20dp"
            android:text="@string/isw_title_choose_category"
            android:layout_marginLeft="20dp" />

        <View
            android:id="@+id/isw_view"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:background="@drawable/isw_card_background"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toTopOf="@+id/isw_guideline"/>

        <LinearLayout
            android:orientation="vertical"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintTop_toTopOf="@+id/isw_view">
                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="50dp"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent">


                        <GridLayout
                            android:id="@+id/category_container"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="10dp"
                            app:layout_constraintTop_toTopOf="parent"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            android:rowCount="2"
                            android:columnCount="2"
                            android:useDefaultMargins="true"
                            android:alignmentMode="alignMargins"
                            android:columnOrderPreserved="false">

                            <androidx.cardview.widget.CardView
                                android:id="@+id/isw_bill_airtime"
                                style="@style/Theme.MaterialComponents.Light"
                                android:layout_width="150dp"
                                android:layout_height="157dp"
                                android:layout_marginTop="12dp"
                                android:layout_marginBottom="24dp"
                                android:clickable="true"
                                android:focusable="true"
                                app:cardCornerRadius="10dp"
                                app:cardElevation="3dp"
                                app:strokeWidth="0.2dp">
                                <androidx.constraintlayout.widget.ConstraintLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:orientation="vertical"
                                    android:padding="5dp">

                                    <com.google.android.material.textview.MaterialTextView
                                        android:id="@+id/isw_airtime_text"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:text="@string/isw_airtime"
                                        android:gravity="center"
                                        android:textColor="@android:color/black"
                                        android:textSize="20sp"
                                        android:drawableTop="@drawable/ic_airtime"
                                        android:textStyle="bold"
                                        app:layout_constraintEnd_toEndOf="parent"
                                        app:layout_constraintStart_toStartOf="parent"
                                        app:layout_constraintTop_toTopOf="parent"
                                        app:layout_constraintBottom_toBottomOf="parent"/>

                                </androidx.constraintlayout.widget.ConstraintLayout>

                            </androidx.cardview.widget.CardView>

                            <androidx.cardview.widget.CardView
                                android:id="@+id/isw_bill_utility"
                                style="@style/Theme.MaterialComponents.Light"
                                android:layout_width="150dp"
                                android:layout_height="157dp"
                                android:layout_marginTop="12dp"
                                android:layout_marginBottom="24dp"
                                android:clickable="true"
                                android:focusable="true"
                                app:cardCornerRadius="10dp"
                                app:cardElevation="3dp"
                                app:strokeWidth="0.2dp">
                                <androidx.constraintlayout.widget.ConstraintLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:orientation="vertical"
                                    android:padding="5dp">

                                    <com.google.android.material.textview.MaterialTextView
                                        android:id="@+id/isw_utility_text"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:text="@string/isw_utility_bills"
                                        android:gravity="center"
                                        android:textColor="@android:color/black"
                                        android:textSize="20sp"
                                        android:drawableTop="@drawable/ic_utilities"
                                        android:textStyle="bold"
                                        app:layout_constraintEnd_toEndOf="parent"
                                        app:layout_constraintStart_toStartOf="parent"
                                        app:layout_constraintTop_toTopOf="parent"
                                        app:layout_constraintBottom_toBottomOf="parent"/>

                                </androidx.constraintlayout.widget.ConstraintLayout>

                            </androidx.cardview.widget.CardView>

                            <androidx.cardview.widget.CardView
                                android:id="@+id/isw_bill_cable_tv"
                                style="@style/Theme.MaterialComponents.Light"
                                android:layout_width="150dp"
                                android:layout_height="157dp"
                                android:layout_marginTop="12dp"
                                android:layout_marginBottom="24dp"
                                android:clickable="true"
                                android:focusable="true"
                                app:cardCornerRadius="10dp"
                                app:cardElevation="3dp"
                                app:strokeWidth="0.2dp">
                                <androidx.constraintlayout.widget.ConstraintLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:orientation="vertical"
                                    android:padding="5dp">

                                    <com.google.android.material.textview.MaterialTextView
                                        android:id="@+id/isw_cable_tv_text"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:text="@string/isw_cable_tv"
                                        android:gravity="center"
                                        android:textColor="@android:color/black"
                                        android:textSize="20sp"
                                        android:drawableTop="@drawable/ic_movie"
                                        android:textStyle="bold"
                                        app:layout_constraintEnd_toEndOf="parent"
                                        app:layout_constraintStart_toStartOf="parent"
                                        app:layout_constraintTop_toTopOf="parent"
                                        app:layout_constraintBottom_toBottomOf="parent"/>

                                </androidx.constraintlayout.widget.ConstraintLayout>

                            </androidx.cardview.widget.CardView>



<!--                            <androidx.cardview.widget.CardView-->
<!--                                android:id="@+id/isw_bill_mobile_data"-->
<!--                                style="@style/Theme.MaterialComponents.Light"-->
<!--                                android:layout_width="150dp"-->
<!--                                android:layout_height="157dp"-->
<!--                                android:layout_marginTop="12dp"-->
<!--                                android:layout_marginBottom="24dp"-->
<!--                                android:clickable="true"-->
